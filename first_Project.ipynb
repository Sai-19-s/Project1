   "source": [
    "# finding n is a prime or not.\n",
    "n = int(input(\"Enter a number: \"))\n",
    "check = True\n",
    "if n>1:\n",
    "    for i in range(2, n//2+1):\n",
    "        if n//i == 0:\n",
    "            check = False\n",
    "    if check:\n",
    "        print(f\"{n} is a prime number.\")\n",
    "    else:\n",
    "        print(f\"{n} is not a prime number.\")\n",
    "    "
   ]
